		<!--  Nalco Settings -->
		<_parameter _name="domain-name" _xpath="//configuration/Nalco/enVision/SharePoint/add[@key='DomainName']/@value" />
		<_parameter _name="user-name" _xpath="//configuration/Nalco/enVision/SharePoint/add[@key='Username']/@value" />
		<_parameter _name="password" _xpath="//configuration/Nalco/enVision/SharePoint/add[@key='Password']/@value" _encrypted="false" />
		<_parameter _name="service-url" _xpath="//configuration/Nalco/enVision/SharePoint/add[@key='ServiceUrl']/@value" />
		<_parameter _name="web-url" _xpath="//configuration/Nalco/enVision/SharePoint/add[@key='WebUrl']/@value" />
		
		
    </nodes>
